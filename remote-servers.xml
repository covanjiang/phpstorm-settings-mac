<application>
  <component name="RemoteServers">
    <remote-server name="Docker" type="docker">
      <configuration>
        <option name="apiUrl" value="unix:///var/run/docker.sock" />
        <option name="certificatesPath" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="/Users" />
              <option name="virtualMachinePath" value="/Users" />
            </DockerPathMappingImpl>
            <DockerPathMappingImpl>
              <option name="localPath" value="/home/mac/code/admin-apkfab-api" />
              <option name="virtualMachinePath" value="/var/www/admin-apkfab-api" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
    <remote-server name="DockerForMac" type="docker">
      <configuration>
        <option name="apiUrl" value="unix:///var/run/docker.sock" />
        <option name="certificatesPath" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="$USER_HOME$/code" />
              <option name="virtualMachinePath" value="/var/www" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
  </component>
</application>